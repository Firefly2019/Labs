#include<iostream>
#include<string>
#include<vector>
#include"Course.h"

using namespace std;


int main(){

 const int size=3;

 Course* ptr[size];
 

 

  for(int i=0; i<3; i++){
     ptr[i] = new Course;    
    
    
  }

  ptr[0]->setCourseNumber(70503);
  ptr[0]->setCourseName("CS211");
   ptr[0]->setNumberOfCredits(4);

  ptr[0]->print();
 






  delete ptr[size];

  return 0;

}
